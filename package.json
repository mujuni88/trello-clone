{
  "name": "trello-clone",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.16.0",
    "mobx": "^4.1.1",
    "mobx-decorators": "^4.1.0",
    "mobx-react": "^5.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.7.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.14.0",
    "serve": "^7.1.6",
    "styled-components": "^3.2.5",
    "styled-system": "^2.2.5",
    "styled-tools": "^0.5.0"
  },
  "scripts": {
    "start": "react-app-rewired start --scripts-version react-scripts-ts",
    "now-start": "serve --single ./build",
    "build": "react-app-rewired build --scripts-version react-scripts-ts",
    "deploy": "aws s3 cp --recursive ./build s3://jb-trello-clone",
    "test": "react-app-rewired test --env=jsdom --scripts-version react-scripts-ts",
    "test:all": "jest --config jest.config.js",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.2",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewire-lodash": "^2.0.0",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewire-typescript": "^2.0.2",
    "react-app-rewired": "^1.5.2",
    "ts-import-plugin": "^1.4.4",
    "ts-jest": "^22.4.2",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.8.1"
  }
}
